  "scripts": {
    "client-install": "npm install --prefix client",
    "tt": "node src/tt.js",
    "start": "node server.js",
    "watch": "nodemon -w src -w server.js --exec yarn start",
    "client": "npm start --prefix client",
    "dev": "concurrently --kill-others-on-fail \"npm run watch\" \"npm run client\"",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongoose": "^5.3.7",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "recursive-readdir-sync": "^1.0.6",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "morgan": "^1.9.1",
    "nodemon": "^1.18.5"
  }
}
